generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model User {
  id        String   @id @default(uuid())
  name      String
  email     String   @unique
  password  String
  admin     Boolean
}

model Product {
  id        String  @id @default(uuid())
  tipo      String
  caimento  String
  material  String
  tamanho   String
  preco     Float
}
